    <body class="index-opt-5">
        <div class="wrapper">
            <form id="block-search-mobile" method="get" class="block-search-mobile">
                <div class="form-content">
                    <div class="control">
                        <a href="#" class="close-block-serach"><span class="icon fa fa-times"></span></a>
                        <input type="text" name="search" placeholder="Search" class="input-subscribe">
                        <button type="submit" class="btn search">
                            <span><i class="fa fa-search" aria-hidden="true"></i></span>
                        </button>
                    </div>
                </div>
            </form>
            <!-- HEADER -->
            {% block header %}
                {% include "store/shared/header.premium.html.twig"  %}
            {% endblock %}
            <!-- MAIN -->
            <main class="site-main" id="site-main">
                {% block breadcrumbs %}
                    <div class="container">
                        <ol class="breadcrumb-page">
                        </ol>
                    </div>
                {% endblock %}
                {% include 'shared/feedback.dialog.html.twig' %}
                {% include 'store/shared/add.to.cart.feedback.dialog.html.twig' %}

                {% include 'shared/feedback.html.twig' %}
                {% block mqrBlock %}    
                    {% include 'store/shared/mqr.dialog.html.twig' %}
                    {% include 'store/shared/mqr.html.twig' %}
                {% endblock %}
                {% block content %}                 
                {% endblock  %}
            </main><!-- end MAIN -->
            <!-- FOOTER -->
            {% block footer %}
                {% include 'store/shared/footer.opt1.html.twig' %}
            {% endblock %}
            <!--back-to-top  -->
            <!-- <a href="#" class="back-to-top">
                <i aria-hidden="true" class="fa fa-angle-up"></i>
            </a> -->
        </div>
        <a href="#" id="scrollup" title="Scroll to Top">Scroll</a>
        {% block javascripts %}
            {% include 'store/shared/scripts.html.twig' %}
            <script src="{{asset('js/hubstore.js')}}"></script>   
        {% endblock %}
